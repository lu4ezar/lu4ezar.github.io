{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "no-param-reassign": ["error", { "props": false }],
    "react/jsx-props-no-spreading": ["off"],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": ["**/*.stories.jsx", "**/*.test.jsx"]
      }
    ]
  }
}
